{
  "name": "gemini-git-agent",
  "productName": "Gemini Git Agent",
  "description": "AI-Powered Git Client",
  "author": "Gemini Team",
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
